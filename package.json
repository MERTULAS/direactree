{
  "name": "direactree",
  "version": "0.1.4",
  "description": "React UI component library for directory tree",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MERTULAS/direactree.git"
  },
  "homepage": "https://direactree-documentation.vercel.app/",
  "bugs": {
    "url": "https://github.com/MERTULAS/direactree/issues"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./next": {
      "require": "./dist/next.js",
      "import": "./dist/next.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "start-storybook -p 6006",
    "test": "jest",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "component",
    "ui",
    "directory",
    "tree",
    "nextjs",
    "react-ui",
    "directory-tree"
  ],
  "author": "Mert ULAS",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "jest": "^28.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.77.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  }
}
